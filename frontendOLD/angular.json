{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "orgRegistration": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "inlineStyle": true,
          "inlineTemplate": true,
          "skipTests": true,
          "flat": true
        }
      },
      "root": "projects/org-registration",
      "sourceRoot": "projects/org-registration/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/org-registration",
            "index": "projects/org-registration/src/index.html",
            "main": "projects/org-registration/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/org-registration/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/org-registration/src/favicon.ico",
              "projects/org-registration/src/assets",
              {
                "glob": "**/*",
                "input": "projects/shared/src/lib/assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/ngx-spinner/animations/square-jelly-box.css",
              "projects/shared/src/lib/scss/app.scss",
              "projects/org-registration/src/scss/app.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "orgRegistration:build:production"
            },
            "development": {
              "browserTarget": "orgRegistration:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "browserTarget": "frontend:build",
            "proxyConfig": "proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "orgRegistration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/org-registration/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/org-registration/src/favicon.ico",
              "projects/org-registration/src/assets",
              {
                "glob": "**/*",
                "input": "projects/shared/src/lib/assets/",
                "output": "./assets"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/ngx-spinner/animations/square-jelly-box.css",
              "projects/shared/src/lib/scss/app.scss",
              "projects/org-registration/src/scss/app.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          }
        }
      }
    },
    "scrApplication": {
