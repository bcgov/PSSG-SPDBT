{
  "name": "spd",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "gen-api": "ng-openapi-gen -i https://dev-spd-screening-portal.apps.emerald.devops.gov.bc.ca/swagger/v1/swagger.json -o ./src/app/api --ignoreUnusedModels false",
    "gen-api-all-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api",
    "gen-api-tags-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api --includeTags $TAGS --removeStaleFiles false",
    "start": "ng serve --port 4200 --proxy-config proxy.conf.json",
    "startlocal": "npm run gen-api-all-local && ng serve --port 4200 --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/cdk": "^19.1.2",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/forms": "^19.1.4",
    "@angular/material": "^19.1.2",
    "@angular/material-moment-adapter": "^19.1.2",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "@angular/router": "^19.1.4",
    "@bcgov/bc-sans": "^2.1.0",
    "@ngneat/overview": "^6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngxpert/hot-toast": "^4.1.1",
    "angular-oauth2-oidc": "^19.0.0",
    "bootstrap": "^5.2.3",
    "jquery": "^3.7.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.29.4",
    "ng-openapi-gen": "^0.52.0",
    "ng-recaptcha-2": "^15.0.2",
    "ngx-mask": "^19.0.6",
    "ngx-spinner": "^17.0.0",
    "rxjs": "~7.8.0",
    "spd": "file:",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular-eslint/builder": "^19.0.2",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular-eslint/schematics": "^19.0.2",
    "@angular-eslint/template-parser": "^19.0.2",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.1.4",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "~9.19.0",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.1.2",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3"
  }
}
