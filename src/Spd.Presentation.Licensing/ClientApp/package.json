{
  "name": "spd",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "gen-api": "ng-openapi-gen -i https://frontend-dev.apps.emerald.devops.gov.bc.ca/api/openapi/v1/openapi.json -o ./src/app/api --ignoreUnusedModels false",
    "gen-api-all-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api",
    "gen-api-tags-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api --includeTags $TAGS --removeStaleFiles false",
    "start": "ng serve --port 4200 --proxy-config proxy.conf.json",
    "startlocal": "npm run gen-api-all-local && ng serve --port 4200 --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/material": "^17.3.10",
    "@angular/material-moment-adapter": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@bcgov/bc-sans": "^2.1.0",
    "@ngneat/hot-toast": "^7.0.0",
    "@ngneat/overview": "^6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "angular-oauth2-oidc": "^15.0.1",
    "bootstrap": "^5.2.3",
    "jquery": "^3.7.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "ng-openapi-gen": "^0.51.0",
    "ng-recaptcha": "^12.0.1",
    "ngx-dropzone": "^3.1.0",
    "ngx-mask": "^16.0.4",
    "ngx-spinner": "^16.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^17.3.0",
    "prettier": "^2.8.8",
    "typescript": "^5.4.5"
  }
}