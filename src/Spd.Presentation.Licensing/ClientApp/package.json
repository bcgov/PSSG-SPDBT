{
  "name": "spd",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "gen-api": "ng-openapi-gen -i https://frontend-dev.apps.emerald.devops.gov.bc.ca/api/openapi/v1/openapi.json -o ./src/app/api --ignoreUnusedModels false",
    "gen-api-all-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api",
    "gen-api-tags-local": "ng-openapi-gen -i http://localhost:5114/swagger/v1/swagger.json -o ./src/app/api --includeTags $TAGS --removeStaleFiles false",
    "start": "ng serve --port 4200 --proxy-config proxy.conf.json",
    "startlocal": "npm run gen-api-all-local && ng serve --port 4200 --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.11",
    "@angular/cdk": "^18.2.9",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.11",
    "@angular/material": "^18.2.12",
    "@angular/material-moment-adapter": "^18.2.12",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.11",
    "@bcgov/bc-sans": "^2.1.0",
    "@ngneat/overview": "^6.0.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngxpert/hot-toast": "^3.0.0",
    "angular-oauth2-oidc": "^17.0.2",
    "bootstrap": "^5.2.3",
    "jquery": "^3.7.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.29.4",
    "ng-openapi-gen": "^0.52.0",
    "ng-recaptcha-2": "^14.0.0",
    "ngx-mask": "^18.0.4",
    "ngx-spinner": "^17.0.0",
    "rxjs": "~7.8.0",
    "spd": "file:",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-eslint/builder": "18.4.0",
    "@angular-eslint/eslint-plugin": "18.4.0",
    "@angular-eslint/eslint-plugin-template": "18.4.0",
    "@angular-eslint/schematics": "18.4.0",
    "@angular-eslint/template-parser": "18.4.0",
    "@angular/cli": "~18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
